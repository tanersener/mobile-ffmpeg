.SH NAME
gnutls_privkey_import_x509 \- API function
.SH SYNOPSIS
.B #include <gnutls/abstract.h>
.sp
.BI "int gnutls_privkey_import_x509(gnutls_privkey_t " pkey ", gnutls_x509_privkey_t " key ", unsigned int " flags ");"
.SH ARGUMENTS
.IP "gnutls_privkey_t pkey" 12
