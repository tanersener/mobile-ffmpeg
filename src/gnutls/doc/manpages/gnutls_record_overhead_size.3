.SH NAME
gnutls_record_overhead_size \- API function
.SH SYNOPSIS
.B #include <gnutls/gnutls.h>
.sp
.BI "size_t gnutls_record_overhead_size(gnutls_session_t " session ");"
.SH ARGUMENTS
.IP "gnutls_session_t session" 12
