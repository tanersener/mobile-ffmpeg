.SH NAME
gnutls_sign_algorithm_get \- API function
.SH SYNOPSIS
.B #include <gnutls/gnutls.h>
.sp
.BI "int gnutls_sign_algorithm_get(gnutls_session_t " session ");"
.SH ARGUMENTS
.IP "gnutls_session_t session" 12
